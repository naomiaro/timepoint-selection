{
  "name": "timepoint-selection",
  "version": "0.2.3",
  "description": "Key controlled time input duration selection",
  "main": "index.js",
  "scripts": {
    "compile": "babel main.js --out-file index.js",
    "deploy": "webpack && gh-pages -d dist -b gh-pages -o origin",
    "start": "node_modules/webpack-dev-server/bin/webpack-dev-server.js --content-base dist/ --colors --inline --host 0.0.0.0",
    "version": "webpack && git add -A dist",
    "postversion": "git push && git push --tags",
    "prepublishOnly": "npm run compile"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/naomiaro/timepoint-selection.git"
  },
  "keywords": [
    "time",
    "point",
    "selection",
    "duration",
    "key",
    "input",
    "format"
  ],
  "author": "Naomi Aro <naomiaro@gmail.com> (http://naomiaro.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/naomiaro/timepoint-selection/issues"
  },
  "homepage": "https://github.com/naomiaro/timepoint-selection#readme",
  "dependencies": {
    "lodash.assign": "^4.0.9",
    "moment": "^2.13.0",
    "moment-duration-format": "^1.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.3.13",
    "gh-pages": "^0.11.0",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.0"
  }
}
